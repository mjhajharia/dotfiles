{
   "action": {
   },
   "background": {
      "service_worker": "worker.js"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "description": "A powerful optical character recognition (OCR) extension to capture and convert images to text",
   "homepage_url": "https://webextension.org/listing/ocr.html",
   "host_permissions": [ "https://tessdata.projectnaptha.com/*.gz", "https://github.com/naptha/tessdata/blob/gh-pages/*.gz?raw=true" ],
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "512": "data/icons/512.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "OCR - Image Reader",
   "permissions": [ "storage", "unlimitedStorage", "activeTab", "notifications", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.5",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "/data/engine/index.html" ]
   } ]
}
